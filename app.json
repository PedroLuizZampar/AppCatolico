{
  "expo": {
    "name": "Sanctus",
    "slug": "AppCatolico",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/nova-logo-app.jpg",
    "scheme": "appcatolico",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pedro_luiz_zampar.AppCatolico"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/nova-logo-app.jpg"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.pedro_luiz_zampar.AppCatolico"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/nova-logo-app.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/nova-logo-app.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7da70aec-3eec-4fdc-a9a0-98eb5f785dd8"
      }
    },
    "splash": {
      "image": "./assets/nova-logo-app.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    }
  }
}
